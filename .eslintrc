{
  "root": true, // Don't traverse ancestor directories looking for .eslintrc[.*] files to merge.
  "env": {
    "es6": true,
    "browser": true,
    "node": true
  },
  "rules": {
    "curly": 0,
    "comma-dangle": ["warn", "always-multiline"],
    "comma-spacing": 0,
    "eqeqeq": [2, "allow-null"],
    "key-spacing": 0,
    "no-dupe-args": 2,
    "no-dupe-keys": 2,
    "no-duplicate-case": 2,
    "no-empty": 1,
    "no-ex-assign": 2,
    "no-extra-boolean-cast": 2,
    "no-extra-parens": 1,
    "no-extra-semi": 2,
    "no-func-assign": 2,
    "no-multi-spaces": 2,
    "no-obj-calls": 2,
    "no-template-curly-in-string": 2,
    "no-undef": 2,
    "no-underscore-dangle": 0,
    "no-unused-expressions": 0,
    "no-redeclare": 2,
    "no-return-assign": 2,
    "no-shadow": 0,
    "no-shadow-restricted-names": 0,
    "no-extend-native": 0,
    "no-var": 2,
    "indent": ["warn",4, { "SwitchCase": 1 }],
    "new-cap": 0,
    "quotes": 0,
    "semi-spacing": 0,
    "space-unary-ops": 0,
    "space-infix-ops": 0,
    "consistent-return": 0,
    "strict": 0,
    "no-cond-assign": ["error", "always"],
    "no-console": "warn",
  },
  "parser": "babel-eslint",
  "plugins": [
    "react"
  ],
  "ecmaFeatures": {
    "jsx": true
  }
}
